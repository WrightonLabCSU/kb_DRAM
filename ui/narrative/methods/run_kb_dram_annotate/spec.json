{
    "ver": "0.0.2",
    "authors": [
        "michael_shaffer"
    ],
    "contact": "",
    "categories": ["active", "annotation"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "assembly_input_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomeAnnotations.Assembly", "KBaseGenomes.ContigSet", "KBaseSets.AssemblySet" ]
            }
        },
        {
            "id": "is_metagenome",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		        "is_output_name": false
            }
        },
        {
            "id": "min_contig_size",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "2500" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0
            }
        },
        {
            "id": "trans_table",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "11" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 1,
                "max_integer": 25
            }
        },
        {
            "id": "bitscore",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "60" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0
            }
        },
        {
            "id": "rbh_bitscore",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "350" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSearch.GenomeSet", "KBaseSets.GenomeSet" ],
		        "is_output_name": true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_DRAM",
            "method": "run_kb_dram_annotate",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "assembly_input_ref",
                    "target_property": "assembly_input_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "desc",
                    "target_property": "desc"
                },{
                    "input_parameter": "min_contig_size",
                    "target_property": "min_contig_size"
                },{
                    "input_parameter": "trans_table",
                    "target_property": "trans_table"
                },{
                    "input_parameter": "bitscore",
                    "target_property": "bitscore"
                },{
                    "input_parameter": "rbh_bitscore",
                    "target_property": "rbh_bitscore"
                },{
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
